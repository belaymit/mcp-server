{
  "name": "mcp-proxy-server",
  "version": "0.1.0",
  "description": "A lightweight MCP proxy server that routes requests to multiple downstream MCP servers",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node mcp_proxy_server/main.ts",
    "dev:ui": "ts-node mcp_proxy_server/app.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint mcp_proxy_server/**/*.ts",
    "format": "prettier --write mcp_proxy_server/**/*.ts"
  },
  "keywords": [
    "mcp",
    "proxy",
    "server",
    "typescript"
  ],
  "author": "AI Protocol Engineer",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.0",
    "socket.io": "^4.7.0",
    "winston": "^3.11.0",
    "ws": "^8.14.0",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "nock": "^13.4.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.2.0"
  }
}
